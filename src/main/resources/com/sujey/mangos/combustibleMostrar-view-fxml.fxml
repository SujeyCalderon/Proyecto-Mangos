<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sujey.mangos.controllers.CombustibleMostrarController">
   <children>
      <AnchorPane prefHeight="420.0" prefWidth="615.0">
         <children>
            <AnchorPane prefHeight="410.0" prefWidth="618.0">
               <children>
                  <ImageView fitHeight="420.0" fitWidth="620.0">
                     <image>
                        <Image url="@../images/FondoHome.png" />
                     </image>
                  </ImageView>
                  <Button fx:id="agregar" mnemonicParsing="false" onMouseClicked="#MouseClickAgregar" prefHeight="60.0" prefWidth="330.0" style="-fx-background-color: #FFF212; -fx-border-color: black; -fx-border-width: 3;" text="AGREGAR">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="mostrar" layoutX="336.0" mnemonicParsing="false" onMouseClicked="#MouseClickMostrar" prefHeight="60.0" prefWidth="283.0" style="-fx-background-color: #FFF212; -fx-border-color: black; -fx-border-width: 3;" text="MOSTRAR">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="offWindow" layoutX="457.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#MouseClickoffWindow" prefHeight="40.0" prefWidth="112.0" style="-fx-background-color: #ED3237; -fx-background-radius: 10;" text="SALIR" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Button>
                  <Button fx:id="ver" layoutX="51.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#MouseClickVer" prefHeight="40.0" prefWidth="91.0" style="-fx-background-color: #00A859; -fx-background-radius: 10;" text="VER" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Button>
                  <Button fx:id="modificar" layoutX="157.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#MouseClickModificar" prefHeight="40.0" prefWidth="133.0" style="-fx-background-color: #CDC63B; -fx-background-radius: 10;" text="MODIFICAR" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Button>
                  <Button fx:id="eliminar" layoutX="310.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#MouseClickEliminar" prefHeight="40.0" prefWidth="133.0" style="-fx-background-color: #CDC63B; -fx-background-radius: 10;" text="ELIMINAR" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Button>
                  <TableView fx:id="TableCombustible" layoutX="51.0" layoutY="97.0" prefHeight="200.0" prefWidth="518.0">
                    <columns>
                      <TableColumn fx:id="NombreColumn" prefWidth="75.0" text="Nombre" />
                      <TableColumn fx:id="FechaColumn" prefWidth="91.0" text="Fecha" />
                        <TableColumn fx:id="DescripcionColumn" prefWidth="143.0" text="Descripcion" />
                        <TableColumn fx:id="MaquinariaColumn" prefWidth="116.0" text="Maquinaria" />
                        <TableColumn fx:id="CostoColumn" prefWidth="92.0" text="Costo" />
                    </columns>
                  </TableView>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
