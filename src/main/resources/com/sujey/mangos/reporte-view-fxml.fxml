<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sujey.mangos.controllers.ReporteController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@../images/FondoHome.png" />
         </image>
      </ImageView>
      <Button fx:id="Ver" layoutX="256.0" layoutY="334.0" mnemonicParsing="false" onMouseClicked="#MouseClickVer" prefHeight="22.0" prefWidth="91.0" style="-fx-background-color: green; -fx-background-radius: 10;" text="VER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <TableView fx:id="TableReportes" layoutX="47.0" layoutY="100.0" prefHeight="200.0" prefWidth="507.0">
        <columns>
          <TableColumn fx:id="VentasColumn" prefWidth="268.0" text="Total de Ventas" />
          <TableColumn fx:id="GastosColumn" prefWidth="238.0" text="Total de Gastos" />
        </columns>
      </TableView>
      <ImageView fitHeight="57.0" fitWidth="600.0">
         <image>
            <Image url="@../images/BarraHome.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="24.0" fitWidth="394.0" layoutX="150.0" layoutY="17.0">
         <image>
            <Image url="@../images/TituloHome.png" />
         </image>
      </ImageView>
      <Button fx:id="offWindow" layoutX="14.0" layoutY="13.0" mnemonicParsing="false" onMouseClicked="#MouseClickoffWindow" prefHeight="28.0" prefWidth="106.0" style="-fx-background-color: red; -fx-background-radius: 10;" text="SALIR" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
   </children>
</AnchorPane>
