<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sujey.mangos.controllers.GastoController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0">
         <image>
            <Image url="@../images/FondoHome.png" />
         </image>
      </ImageView>
      <Button fx:id="Ver" layoutX="266.0" layoutY="344.0" mnemonicParsing="false" onMouseClicked="#MouseClickVer" prefHeight="22.0" prefWidth="91.0" style="-fx-background-color: green; -fx-background-radius: 10;" text="VER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <ImageView fitHeight="59.0" fitWidth="600.0" layoutY="-4.0">
         <image>
            <Image url="@../images/BarraHome.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="24.0" fitWidth="436.0" layoutX="110.0" layoutY="14.0">
         <image>
            <Image url="@../images/TituloHome.png" />
         </image>
      </ImageView>
      <Button fx:id="offWindow" layoutX="14.0" layoutY="11.0" mnemonicParsing="false" onMouseClicked="#MouseClickoffWindow" prefHeight="31.0" prefWidth="83.0" style="-fx-background-color: red; -fx-background-radius: 10;" text="SALIR" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <TableView fx:id="TableGastos" layoutX="50.0" layoutY="90.0" prefHeight="200.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="ActividadesColumn" prefWidth="104.0" text=" Actividades" />
          <TableColumn fx:id="MedicamentosColumn" prefWidth="128.0" text="Medicamentos" />
            <TableColumn fx:id="CombustiblesColumn" prefWidth="111.0" text="Combustibles" />
            <TableColumn fx:id="SueldoColumn" prefWidth="75.0" text="Sueldo" />
            <TableColumn fx:id="GastosColumn" prefWidth="81.0" text="Gastos" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
